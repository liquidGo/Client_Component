{
  "name": "@jinyouyu/ui",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "dist-demo"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --mode library && npx tsc -p tsconfig.build.json",
    "build:demo": "vite build --mode demo",
    "build:all": "npm run build:lib && npm run build:demo",
    "prebuild": "npm run type-check && npm run lint",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run prebuild && npm run build:all"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "antd": ">=5.0.0",
    "dayjs": ">=1.11.0",
    "@ant-design/icons": ">=5.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@ant-design/icons": "^6.0.2",
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "antd": "^5.27.4",
    "dayjs": "^1.11.18",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "less": "^4.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  },
  "keywords": [
    "react",
    "typescript",
    "component-library",
    "editable",
    "compound-pattern",
    "render-props",
    "antd"
  ],
